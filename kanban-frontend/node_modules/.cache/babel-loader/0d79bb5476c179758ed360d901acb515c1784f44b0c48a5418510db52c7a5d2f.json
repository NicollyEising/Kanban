Board',\r\n  data() {\r\n    return {\r\n      board: null,\r\n      columnNames: ['A Fazer', 'Fazendo', 'Feito'],\r\n    }\r\n  },\r\n  methods: {\r\n    async loadBoard() {\r\n      try {\r\n        const response = await axios.get('http://localhost:8000/api/board/2/')\r\n        this.board = response.data\r\n      } catch (error) {\r\n        console.error('Erro ao carregar o board:', error)\r\n      }\r\n    },\r\n    filteredCards(columnName) {\r\n      if (!this.board || !this.board.cards) return []\r\n      return this.board.cards.filter(card => card.status === columnName)\r\n    },\r\n  },\r\n  mounted() {\r\n    this.loadBoard()\r\n  },\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n/* mant√©m seu estilo original */\r\n</style>\r\n"],"mappings":";;AAyBA,OAAOA,KAAI,MAAO,OAAM;AAExB,eAAe;EACbC,IAAI,EAAE,aAAa;EACnBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,IAAI;MACXC,WAAW,EAAE,CAAC,SAAS,EAAE,SAAS,EAAE,OAAO;IAC7C;EACF,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,SAASA,CAAA,EAAG;MAChB,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMP,KAAK,CAACQ,GAAG,CAAC,oCAAoC;QACrE,IAAI,CAACL,KAAI,GAAII,QAAQ,CAACL,IAAG;MAC3B,EAAE,OAAOO,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,2BAA2B,EAAEA,KAAK;MAClD;IACF,CAAC;IACDE,aAAaA,CAACC,UAAU,EAAE;MACxB,IAAI,CAAC,IAAI,CAACT,KAAI,IAAK,CAAC,IAAI,CAACA,KAAK,CAACU,KAAK,EAAE,OAAO,EAAC;MAC9C,OAAO,IAAI,CAACV,KAAK,CAACU,KAAK,CAACC,MAAM,CAACC,IAAG,IAAKA,IAAI,CAACC,MAAK,KAAMJ,UAAU;IACnE;EACF,CAAC;EACDK,OAAOA,CAAA,EAAG;IACR,IAAI,CAACX,SAAS,CAAC;EACjB;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}