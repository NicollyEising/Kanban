{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport draggable from 'vuedraggable';\nimport axios from 'axios';\nexport default {\n  components: {\n    draggable\n  },\n  data() {\n    return {\n      columns: [] // será carregado da API Django\n    };\n  },\n  mounted() {\n    this.loadBoard();\n  },\n  methods: {\n    async loadBoard() {\n      const res = await axios.get('http://localhost:8000/api/board/1/');\n      this.columns = res.data.columns;\n    },\n    async onDragEnd(event) {\n      const column = this.columns[event.to.dataset.columnIndex];\n      const cardIds = column.cards.map((card, index) => {\n        return {\n          id: card.id,\n          order: index\n        };\n      });\n      await axios.post('/api/cards/reorder/', {\n        column_id: column.id,\n        cards: cardIds\n      });\n    }\n  }\n};","map":{"version":3,"names":["draggable","axios","components","data","columns","mounted","loadBoard","methods","res","get","onDragEnd","event","column","to","dataset","columnIndex","cardIds","cards","map","card","index","id","order","post","column_id"],"sources":["C:\\Users\\faculdade\\Desktop\\Kanban\\kanban-frontend\\src\\KanbanBoard.vue"],"sourcesContent":["<template>\r\n  <div class=\"kanban\">\r\n    <div class=\"column\" v-for=\"(column, index) in columns\" :key=\"column.id\">\r\n      <h3>{{ column.name }}</h3>\r\n      <draggable\r\n        v-model=\"column.cards\"\r\n        :group=\"'cards'\"\r\n        :data-column-index=\"index\"\r\n        @end=\"onDragEnd\"\r\n      >\r\n        <template #item=\"{ element }\">\r\n          <div class=\"card\">\r\n            {{ element.title }}\r\n          </div>\r\n        </template>\r\n        <template #footer>\r\n          <div v-if=\"column.cards.length === 0\" class=\"empty\">\r\n            Está vazio\r\n          </div>\r\n        </template>\r\n      </draggable>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport draggable from 'vuedraggable';\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  components: { draggable },\r\n  data() {\r\n    return {\r\n      columns: [],  // será carregado da API Django\r\n    };\r\n  },\r\n  mounted() {\r\n    this.loadBoard();\r\n  },\r\n  methods: {\r\n    async loadBoard() {\r\n      const res = await axios.get('http://localhost:8000/api/board/1/')\r\n\r\n      this.columns = res.data.columns;\r\n    },\r\n    async onDragEnd(event) {\r\n      const column = this.columns[event.to.dataset.columnIndex];\r\n      const cardIds = column.cards.map((card, index) => {\r\n        return { id: card.id, order: index };\r\n      });\r\n      await axios.post('/api/cards/reorder/', {\r\n        column_id: column.id,\r\n        cards: cardIds,\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.kanban {\r\n  display: flex;\r\n  gap: 16px;\r\n}\r\n.column {\r\n  background: #f4f4f4;\r\n  padding: 16px;\r\n  width: 250px;\r\n}\r\n.card {\r\n  background: white;\r\n  padding: 8px;\r\n  margin-bottom: 8px;\r\n  border: 1px solid #ccc;\r\n}\r\n\r\n.empty {\r\n  color: #888;\r\n  font-style: italic;\r\n  text-align: center;\r\n  padding: 8px;\r\n  background: #fff;\r\n  border: 1px dashed #ccc;\r\n}\r\n</style>\r\n"],"mappings":";;AA2BA,OAAOA,SAAQ,MAAO,cAAc;AACpC,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,UAAU,EAAE;IAAEF;EAAU,CAAC;EACzBG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,EAAE,CAAG;IAChB,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,SAAS,CAAC,CAAC;EAClB,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,SAASA,CAAA,EAAG;MAChB,MAAME,GAAE,GAAI,MAAMP,KAAK,CAACQ,GAAG,CAAC,oCAAoC;MAEhE,IAAI,CAACL,OAAM,GAAII,GAAG,CAACL,IAAI,CAACC,OAAO;IACjC,CAAC;IACD,MAAMM,SAASA,CAACC,KAAK,EAAE;MACrB,MAAMC,MAAK,GAAI,IAAI,CAACR,OAAO,CAACO,KAAK,CAACE,EAAE,CAACC,OAAO,CAACC,WAAW,CAAC;MACzD,MAAMC,OAAM,GAAIJ,MAAM,CAACK,KAAK,CAACC,GAAG,CAAC,CAACC,IAAI,EAAEC,KAAK,KAAK;QAChD,OAAO;UAAEC,EAAE,EAAEF,IAAI,CAACE,EAAE;UAAEC,KAAK,EAAEF;QAAM,CAAC;MA